<h1 id="signinpagetitle" >Sign In</h1>

<%= form_for(:session, :url => sessions_path) do |f| %>
  <div class="field">
  	<%= f.label :email %> <br />
	<%= f.text_field :email %>
  </div>
  <div id="signinpassword" class="field">
  	<%= f.label :password %> <br />
	<%= f.password_field :password %>
  </div>
  <div class="actions">
  	<%= f.submit "Sign In" %> 
  </div>
<% end %>

<p id="signupsignin"> New user? <%=  link_to "Sign Up!", signup_path %></p>
